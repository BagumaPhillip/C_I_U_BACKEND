generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}





model LecturerSignUp {
  id         Int    @id @default(autoincrement())
  first_name String
  last_name  String
  email      String @unique
  role       String
  password   String
}


// Student registration

model users {
  id             Int      @id @default(autoincrement())
  name           String
  email          String   @unique
  program         String
  createdAt      DateTime @default(now())
  lastLogin      DateTime?
  registrationNo String   @unique
  password       String
  role          String
}
